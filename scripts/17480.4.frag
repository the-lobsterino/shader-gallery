  color *= sin( time / 10.0 ) * 0.5;
  */

  //rgb = vec3(1.0) * color / 100.0;
  
  dx = x - mod(randRect.x + nsin(time), 1.0)*nsin(time*i/120.0);
  dy = y - mod(randRect.y + nsin(time), 1.0)*nsin(time*(100.0-i)/320.0);

  dist = sqrt(dx * dx + dy * dy);

  if (dist < 0.05) {
    float e = 1.0 / dist / 500.0;
    rgb.r = e*nsin(time)*randColor.x; //time * randColor[0] * sin(x) / 100.0;  
    rgb.g = e*randColor.y;
    rgb.b = e*randColor.z;
  }

  return rgb;
}

