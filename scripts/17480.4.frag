vec3 doRandom(vec2 p, float i, vec3 randColor, vec4 randRect) {
  float dx = 0.0;
  float dy = 0.0;
  float dist = 0.0;
  float x = p.x;
  float y = p.y;
  vec3 rgb = vec3(0.0);
  float color = 0.0;

  /*color += sin( p.x * cos( time * randColor[0] ) * 80.0 * randRect[0] ) + cos( p.y * cos( time / 15.0 ) * 10.0 );
  color += sin( p.y * sin( time * randColor[1] ) * 40.0 * randRect[1]) + cos( p.x * sin( time / 25.0 ) * 40.0 );
