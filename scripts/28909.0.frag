void cp(vec2 origin, vec2 unitx, float low, float high, float k)
{
// find closest point on parabola

	vec2 unity = vec2(-unitx.y, unitx);
	vec2 oc = pos - origin;
	vec2 at;
	at.x = dot(oc, unitx);
	at.y = dot(oc, unity);

	float mid;
	for(int c=0;c<9;++c)
	{
		mid = (low + high) * .5;
		float kmid = k * mid;
		vec2 dir = vec2(1.0, 2.0 * kmid);
		vec2 off = vec2(at.x - mid, at.y - mid * kmid);
		if(dot(dir, off)<0.0)
			high=mid;
		else
			low=mid;
	}
	float tx, ty;
	tx = mid;
	ty = k * tx * tx;
	vec2 center = origin + tx*unitx + ty*unity;
	float d = length(center-pos);
	if(d < best.z)
	{
		best.xy = center;
		best.z = d;
	}
}

