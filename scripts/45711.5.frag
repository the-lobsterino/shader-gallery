float bezier2F(float t, float p0, float p1, float p2) {
	return pow((1.0 - t), 2.0) * p0 + 2.0 * t * (1.0 - t) * p1 + pow(t, 2.0) * p2;
}

vec3 bezier2V3(float t, vec3 c0, vec3 c1, vec3 c2) {
	return vec3(bezier2F(t, c0.x, c1.x, c2.x), bezier2F(t, c0.y, c1.y, c2.y), bezier2F(t, c0.z, c1.z, c2.z));
}


float bezier3F(float t, float p0, float p1, float p2, float p3) {
	return pow((1.0 - t), 3.0) * p0 + 3.0 * t * pow((1.0 - t), 2.0) * p1 + 3.0 * pow(t, 2.0) * (1.0 - t) * p2 + pow(t, 3.0) * p3;;
}

vec3 bezier3V3(float t, vec3 c0, vec3 c1, vec3 c2, vec3 c3) {
	return vec3(bezier3F(t, c0.x, c1.x, c2.x, c3.x), bezier3F(t, c0.y, c1.y, c2.y, c3.y), bezier3F(t, c0.z, c1.z, c2.z, c3.z));
}

float linearF(float t, float p0, float p1) {
	return (1.0 - t) * p0 + t * p1;	
}

vec3 linearV3(float t, vec3 c0, vec3 c1) {
	return vec3(linearF(t, c0.x, c1.x), linearF(t, c0.y, c1.y), linearF(t, c0.z, c1.z));
}

